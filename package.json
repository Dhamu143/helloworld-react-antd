{
  "name": "emiketic-starter-react-material",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://emiketic.github.io/emiketic-starter-react-material",
  "dependencies": {
    "@material-ui/core": "1.5.1",
    "@material-ui/icons": "2.0.3",
    "gh-pages": "1.2.0",
    "promise.prototype.finally": "3.1.0",
    "prop-types": "15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "redux-localstorage": "1.0.0-rc5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --ext .js ./",
    "lint:critical": "eslint --no-eslintrc --config .eslintrc.critical.json ./",
    "format": "npm run format:all ; npm run format:js",
    "format:all": "prettier --ignore-path .gitignore ./**/*.{md,json,js,css,scss} --write",
    "format:js": "eslint --ignore-path .gitignore --ext .js --fix ./",
    "build": "react-scripts build",
    "deploy:github": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile ./localStoragePolyfill.js"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "debug": "3.1.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.14.2"
  }
}
